{
  "name": "n8n-nodes-roundrobin",
  "version": "0.3.1",
  "description": "n8n node to store and retrieve messages in a round-robin fashion, particularly for LLM conversation loops with multiple personas",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "round-robin",
    "llm",
    "conversation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JamesFincher/n8n-nodes-roundrobin",
  "author": {
    "name": "James Fincher",
    "email": "james@fincher.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesFincher/n8n-nodes-roundrobin.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepare-publish": "node prepare-publish.js",
    "prepublishOnly": "npm run prepare-publish"
  },
  "files": [
    "dist",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/RoundRobin/RoundRobin.node.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "typescript": "~4.6.4"
  },
  "dependencies": {
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0"
  }
}
