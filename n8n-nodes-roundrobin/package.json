{
  "name": "n8n-nodes-roundrobin",
  "version": "0.9.0",
  "description": "n8n node to store and retrieve messages in a round-robin fashion, particularly for LLM conversation loops with multiple personas. Uses Airtable for persistent storage.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "round-robin",
    "conversation",
    "chat",
    "llm",
    "openai",
    "claude",
    "gemini",
    "airtable",
    "persistence"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JamesFincher/n8n-nodes-roundrobin",
  "author": {
    "name": "James Fincher",
    "email": "jamesfincher@finch-ai.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesFincher/n8n-nodes-roundrobin.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint --no-eslintrc 'nodes/**/*.ts' package.json",
    "lintfix": "eslint --no-eslintrc 'nodes/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AirtableApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RoundRobin/RoundRobin.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0"
  }
}
